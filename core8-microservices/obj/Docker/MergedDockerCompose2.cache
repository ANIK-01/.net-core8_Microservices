name: dockercompose6891831645514535305
services:
  catalogdb:
    container_name: catalogdb
    environment:
      POSTGRES_DB: CatalogDb
      POSTGRES_PASSWORD: admin
      POSTGRES_USER: postgres
    image: postgres
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5432
        published: "2345"
        protocol: tcp
    restart: always
    volumes:
      - type: volume
        source: postgres_catalog
        target: /var/lib/postgresql/data
        volume: {}
      - type: bind
        source: D:/projects_all
        target: /var/lib/postgresql/Catalog_db
        bind:
          create_host_path: true
  pgadmin:
    container_name: pgadmin4
    environment:
      PGADMIN_DEFAULT_EMAIL: admin@admin.com
      PGADMIN_DEFAULT_PASSWORD: admin
    image: dpage/pgadmin4
    networks:
      catalog_network: null
    ports:
      - mode: ingress
        target: 80
        published: "5544"
        protocol: tcp
    restart: always
  redis_stack:
    container_name: redis_stack
    environment:
      REDIS_PASSWORD: ""
    image: redis/redis-stack:latest
    networks:
      catalog_network: null
    ports:
      - mode: ingress
        target: 8001
        published: "6380"
        protocol: tcp
    restart: always
  seq:
    command:
      - seq
      - config
      - --enable-https
      - --https-port=443
      - --certificate-path=/etc/seq/cert/cert.pem
      - --certificate-key-path=/etc/seq/cert/key.pem
    container_name: seq
    environment:
      ACCEPT_EULA: "Y"
      SEQ_FQDN: seq.local
      SEQ_TRUSTED_PROXIES: 0.0.0.0/0
    image: datalust/seq:latest
    networks:
      catalog_network: null
    ports:
      - mode: ingress
        target: 80
        published: "7001"
        protocol: tcp
      - mode: ingress
        target: 5341
        published: "7002"
        protocol: tcp
    volumes:
      - type: bind
        source: D:\projects_all\TestProjects\.net-core8_Microservices\core8-microservices\path\to\cert
        target: /etc/seq/cert
        read_only: true
        bind:
          create_host_path: true
networks:
  catalog_network:
    name: dockercompose6891831645514535305_catalog_network
    driver: bridge
  default:
    name: dockercompose6891831645514535305_default
volumes:
  postgres_catalog:
    name: dockercompose6891831645514535305_postgres_catalog